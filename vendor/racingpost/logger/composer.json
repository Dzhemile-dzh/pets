{
  "name": "racingpost/logger",
  "description": "Phalcon logger wrapper",
  "authors": [
    {
      "name": "Team Tesla",
      "email": "tesla@racingpost.com"
    }
  ],
  "config": {
    "vendor-dir": "vendor/composer",
    "secure-http": false
  },
  "autoload": {
    "psr-4": {
      "Rp\\": "library/Rp",
      "Phalcon\\": "library/Phalcon"
    }
  },
  "require": {
    "php": ">=5.4",
    "ext-phalcon": ">=1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "version": "3.0.0"
}
