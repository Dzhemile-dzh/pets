    '/{jockeyId}/horses/{year}/{countryCode}/{raceType}/{surface}/{championship}':
        displayName: 'Represents Horses Resource'
        description: 'Jockey profile data related to horses'
        uriParameters:
            jockeyId:
                description: 'Numeric `id` of the Jockey'
                type: integer
                required: true
                example: '88563'
            year:
                description: 'Numeric representation of an year'
                type: integer
                required: false
                example: '2013'
            countryCode:
                description: 'Alphabet short code of country'
                type: string
                required: false
                example: IRE
            raceType:
                description: 'Allowed types of the race types'
                type: string
                required: false
                enum:
                    - flat
                    - jumps
                example: flat
            surface:
                description: 'Allowed types of the surfaces'
                type: string
                required: false
                enum:
                    - turf
                    - aw
                example: turf
            championship:
                description: 'If parameter is specified - statistics will be calculated regarding championship type code'
                type: string
                required: false
                example: championship
