    '/{jockeyId}/statistics/{beginSeasonYear}/{endSeasonYear}/{raceType}/{countryCode}/{statisticsType}/{surface}/{championship}':
        displayName: 'Represents Statistical Resource'
        description: 'Jockey statistical data related to few criteria.'
        uriParameters:
            jockeyId:
                description: 'Numeric `id` of the Jockey'
                type: integer
                required: true
                example: '88563'
            beginSeasonYear:
                description: 'Numeric representation of an year'
                type: integer
                required: false
                example: '2011'
            endSeasonYear:
                description: 'Numeric representation of an year'
                type: integer
                required: false
                example: '2015'
            countryCode:
                description: 'Alphabet short `code` of country'
                type: string
                required: false
                example: GB
            raceType:
                description: 'String `code` of the Race Type'
                type: string
                required: false
                enum:
                    - flat
                    - jumps
                example: flat
            statisticsType:
                description: 'Allowed types of the statistics. Notice `age-of-horse` available only for flat races,`race-category` available for jumps'
                type: string
                required: false
                enum:
                    - course
                    - distance
                    - month
                    - race-type
                    - trainer
                    - race-class
                    - age-of-horse
                    - race-category
                example: distance
            surface:
                description: 'Allowed types of the surfaces'
                type: string
                required: false
                enum:
                    - turf
                    - aw
                example: aw
            championship:
                description: 'If parameter is specified - statistics will be calculated regarding championship start/end dates'
                type: string
                required: false
                example: championship
