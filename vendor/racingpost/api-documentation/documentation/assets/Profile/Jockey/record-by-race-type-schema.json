{
    "$schema":"http://json-schema.org/draft-04/schema#",
    "definitions":{
        "race_type":{
            "required":[
                "NHF",
                "HURDLE",
                "CHASE",
                "2YO AW",
                "2YO TURF",
                "3YO AW",
                "3YO TURF",
                "4YO+ AW",
                "4YO+ TURF"
            ],
            "type":"object",
            "properties":{
                "NHF":{
                    "$ref":"#/definitions/itemTemplate"
                },
                "HURDLE":{
                    "$ref":"#/definitions/itemTemplate"
                },
                "CHASE":{
                    "$ref":"#/definitions/itemTemplate"
                },
                "3YO AW":{
                    "$ref":"#/definitions/itemTemplate"
                },
                "2YO AW":{
                    "$ref":"#/definitions/itemTemplate"
                },
                "4YO+ TURF":{
                    "$ref":"#/definitions/itemTemplate"
                },
                "3YO TURF":{
                    "$ref":"#/definitions/itemTemplate"
                },
                "4YO+ AW":{
                    "$ref":"#/definitions/itemTemplate"
                },
                "2YO TURF":{
                    "$ref":"#/definitions/itemTemplate"
                }
            }
        },
        "itemTemplate":{
            "required":[
                "best_rp_postmark",
                "group_name",
                "horse_name",
                "horse_uid",
                "horses",
                "win_percent",
                "place_1st_number",
                "place_2nd_number",
                "place_3rd_number",
                "place_4th_number",
                "placed",
                "races_number",
                "stake",
                "total_prize",
                "win_prize",
                "euro_win_prize",
                "euro_total_prize"
            ],
            "type":"object",
            "properties":{
                "horses":{
                    "type":"integer"
                },
                "place_2nd_number":{
                    "type":"integer"
                },
                "total_prize":{
                    "type":[
                        "number",
                        "null"
                    ]
                },
                "win_prize":{
                    "type":"number"
                },
                "euro_win_prize":{
                    "type":"number"
                },
                "euro_total_prize":{
                    "type":"number"
                },
                "stake":{
                    "type":[
                        "number",
                        "null"
                    ]
                },
                "place_4th_number":{
                    "type":"integer"
                },
                "races_number":{
                    "type":[
                        "integer",
                        "null"
                    ]
                },
                "group_name":{
                    "type":"string",
                    "maxLength":10
                },
                "placed":{
                    "type":"integer"
                },
                "horse_uid":{
                    "type":[
                        "integer",
                        "null"
                    ]
                },
                "percent":{
                    "type":[
                        "null",
                        "number"
                    ]
                },
                "place_3rd_number":{
                    "type":"integer"
                },
                "best_rp_postmark":{
                    "type":"integer"
                },
                "place_1st_number":{
                    "type":"integer"
                },
                "horse_name":{
                    "type":[
                        "string",
                        "null"
                    ],
                    "maxLength":30
                }
            }
        }
    },
    "required":[
        "data",
        "status"
    ],
    "type":"object",
    "properties":{
        "status":{
            "type":"integer"
        },
        "data":{
            "required":[
                "record_by_race_type",
                "season_info"
            ],
            "type":"object",
            "properties":{
                "record_by_race_type":{
                    "$ref":"#/definitions/race_type"
                },
                "season_info":{
                    "required":[
                        "season_year_begin",
                        "season_year_end",
                        "race_type",
                        "country_code"
                    ],
                    "type":"object",
                    "properties":{
                        "season_year_begin":{
                            "type":"integer"
                        },
                        "season_year_end":{
                            "type":"integer"
                        },
                        "race_type":{
                            "type":"string",
                            "maxLength":5
                        },
                        "country_code":{
                            "type":"string",
                            "maxLength":3
                        }
                    }
                }
            }
        }
    }
}