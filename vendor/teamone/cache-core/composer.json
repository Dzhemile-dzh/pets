{
  "name": "teamone/cache-core",
  "description": "Cache core component",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Denys Solyanyk",
      "email": "denys.solyanyk@racingpost.com"
    },
    {
      "name": "Serhii Kotyk",
      "email": "serhii.kotyk@racingpost.com"
    },
    {
      "name": "Serhii Atiahin",
      "email": "serhii.atiahin@racingpost.com"
    }
  ],
  "config": {
    "vendor-dir": "vendor/composer",
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://library.rp-digital.com/packages/"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-phalcon": ">=1.3",
    "rp/cache-lifetime": "^2.0",
    "rp/content-attributes": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "RP\\Cache\\Core\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.4"
  },
  "autoload-dev": {
    "psr-4": {
      "RP\\Test\\": "test/php/"
    }
  },
  "version": "6.0.1"
}
