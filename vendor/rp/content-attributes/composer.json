{
  "name": "rp/content-attributes",
  "description": "Content attributes component",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Denys Solyanyk",
      "email": "denys.solyanyk@racingpost.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "http://library.rp-digital.com/packages/"
    }
  ],
  "config": {
    "vendor-dir": "vendor/composer",
    "secure-http": false
  },
  "require": {
    "php": ">=7.1",
    "ext-phalcon": ">=1.3",
    "rp/cache-lifetime": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "RP\\ContentAttributes\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.4"
  },
  "autoload-dev": {
    "psr-4": {
      "RP\\Test\\": "test/"
    }
  },
  "version": "3.0.0"
}
